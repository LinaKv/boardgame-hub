{
    "name": "server",
    "version": "0.0.0",
    "private": true,
    "nodemonConfig": {
        "ignore": [
            "myDatabase.json"
        ]
    },
    "scripts": {
        "build": "npx tsc",
        "start": "node dist/app.js",
        "ser": "npm run build && npm run start",
        "server": "nodemon -q dist/app.js",
        "client": "npm run start --prefix ../client",
        "clientinstall": "npm install --prefix ../client",
        "dev": "concurrently \"npx tsc --watch\" \"npm run server\" \"npm run client\" "
    },
    "dependencies": {
        "@types/config": "^0.0.41",
        "@types/cookie-parser": "^1.4.3",
        "@types/morgan": "^1.9.3",
        "config": "^3.3.7",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "express": "~4.16.1",
        "morgan": "~1.9.1",
        "node-json-db": "^1.5.0",
        "pg": "^8.7.3",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.21.1",
        "socket.io": "^4.5.1",
        "toad-scheduler": "^1.6.1",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/node": "^18.0.0",
        "concurrently": "^7.2.2",
        "nodemon": "^2.0.16",
        "typescript": "^4.7.4"
    }
}
